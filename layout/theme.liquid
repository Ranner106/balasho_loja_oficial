  <script>
    document.addEventListener("DOMContentLoaded", function() {
      // Código para o cabeçalho
      const headerContainer = document.querySelector("header .t4s-container");
  
      if (headerContainer) {
        if (window.location.pathname === '/') {
          // Página inicial
          document.addEventListener("scroll", function() {
            if (window.scrollY > 0) {
              headerContainer.classList.add("scrolled");
            } else {
              headerContainer.classList.remove("scrolled");
            }
          });
        } else {
          // Outras páginas
          headerContainer.classList.add("initial");
        }
      }
  
      // Código para animação de entrada das seções
      const sections = document.querySelectorAll('.t4s-section-inner');
  
      const observer = new IntersectionObserver((entries, observer) => {
        entries.forEach(entry => {
          if (entry.isIntersecting) {
            entry.target.classList.add('in-view');
          } else {
            entry.target.classList.remove('in-view');
          }
        });
      }, { threshold: 0.1 });
  
      sections.forEach(section => {
        observer.observe(section);
      });
    });
  </script>
  


